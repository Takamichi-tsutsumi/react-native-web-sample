{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
  ],
  "parser": "babel-eslint",
  "plugins": [
    "flowtype",
    "react",
    "import",
    "jsx-a11y",
  ],
  "rules": {
    "class-methods-use-this": 0,
    "no-use-before-define": 0,
    "array-callback-return": 0,
    "consistent-return": 0,
  }
}
